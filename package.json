{
  "name": "vue-whiteboard-composable",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metacurb/vue-whiteboard-composable.git"
  },
  "description": "A lightweight Vue 3 composable for SVG-based whiteboard drawing with undo/redo and export support.",
  "keywords": [
    "vue",
    "vue3",
    "composable",
    "whiteboard",
    "svg",
    "drawing",
    "d3"
  ],
  "license": "MIT",
  "homepage": "https://github.com/metacurb/vue-whiteboard-composable#readme",
  "bugs": {
    "url": "https://github.com/metacurb/vue-whiteboard-composable/issues"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/"
  },
  "dependencies": {
    "d3": "^7.9.0"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/d3": "^7.4.3",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue-jsx": "^5.1.4",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "~1.48.0",
    "eslint-plugin-vue": "~10.8.0",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "jsdom": "^28.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.48.0",
    "prettier": "3.8.1",
    "typescript": "~5.9.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "packageManager": "pnpm@10.29.3",
  "sideEffects": false
}